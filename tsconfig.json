{
  "compilerOptions": {
    "module": "ES2022",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "outDir": "dist",
    "strict": true,
    "target": "ES2022",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}